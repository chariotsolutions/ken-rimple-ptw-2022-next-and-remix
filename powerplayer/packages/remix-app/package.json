{
  "name": "remix-app",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env PORT=3003 NODE_ENV=production run-s build-css build-remix",
    "build-css": "npm run generate-css -- --minify",
    "build-remix": "remix build",
    "dev": "cross-env PORT=3003 NODE_ENV=development run-p dev-css dev-remix",
    "dev-css": "npm run generate-css -- --watch",
    "dev-remix": "cross-env PORT=3003 remix dev",
    "generate-css": "npx tailwindcss -o ./app/tailwind.css",
    "postinstall": "remix setup node",
    "start": "cross-env PORT=3003 NODE_ENV=production remix-serve build"
  },
  "dependencies": {
    "@remix-run/react": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@remix-run/node": "^1.2.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/pg": "^8.6.5",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "isbot": "^3.4.5",
    "npm-run-all": "^4.1.5",
    "pg": "^8.7.3",
    "pgpromise": "^0.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remix": "^1.2.3",
    "tailwindcss": "^3.0.23",
    "tw-elements": "^1.0.0-alpha12"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "eslint": "^8.9.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
